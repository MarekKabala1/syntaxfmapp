{
	"cli": {
		"version": ">= 16.26.0",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"environment": "development",
			"env": {
				"YOUTUBE_API_KEY": "secret:YOUTUBE_API_KEY",
				"SENTRY_AUTH_TOKEN": "secret:SENTRY_AUTH_TOKEN",
				"APP_VARIANT": "development"
			}
		},
		"preview": {
			"distribution": "internal",
			"channel": "main",
			"environment": "preview",
			"env": {
				"YOUTUBE_API_KEY": "secret:YOUTUBE_API_KEY",
				"SENTRY_AUTH_TOKEN": "secret:SENTRY_AUTH_TOKEN",
				"APP_VARIANT": "production"
			},
			"android": {
				"image": "latest",
				"buildType": "apk"
			},
			"ios": {
				"image": "latest"
			}
		},
		"production": {
			"channel": "production",
			"environment": "production",
			"env": {
				"YOUTUBE_API_KEY": "secret:YOUTUBE_API_KEY",
				"SENTRY_AUTH_TOKEN": "secret:SENTRY_AUTH_TOKEN",
				"APP_VARIANT": "production"
			},
			"android": {
				"image": "latest",
				"buildType": "apk"
			},
			"ios": {
				"image": "latest"
			}
		}
	},
	"submit": {
		"production": {}
	}
}
